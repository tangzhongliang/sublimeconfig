<?xml version="1.0" encoding="utf-8"?>
<!-- リスト画面1列 Eフィードバックなしアイコン(前)あり... -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="687dp"
    android:layout_height="63dp">
    
        <!-- リスト背景 -->
        <ImageView
            android:id="@+id/list_1row_back"
            android:layout_width="687dp"
            android:layout_height="63dp"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dp"
            android:background="@drawable/selector_bt_com_01" />

        <!-- icon_title_ -->
        <ImageView
            android:id="@+id/list_1row_title_radio"
            android:layout_marginLeft="1dp"
            android:layout_marginTop="6dp"
            android:layout_width="64dp"
            android:layout_height="47dp"
            android:src="@drawable/selector_icon_title_radio" />
        
        <!-- タイトル -->
        <TextView
            android:id="@+id/list_1row_title"
            android:layout_marginLeft="64dp"
            android:layout_width="583dp"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:gravity="left"
            android:alpha="0.7"
            android:singleLine="true"
            android:fontFamily="HGSMarugothicM"
            android:textColor="#000000"
            android:textSize="24px" />
        
        <!-- icon_pixt_popup.xs -->
        <ImageView
            android:id="@+id/list_1row_figure"
            android:layout_marginLeft="657dp"
            android:layout_marginTop="47dp"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:src="@drawable/icon_act_pop_01" />
        
</RelativeLayout>
